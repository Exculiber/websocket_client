{
  "examples": {
    "basic_websocket": {
      "uri": "ws://localhost:8080/ws",
      "message": "Hello WebSocket!",
      "timeout": 10
    },
    "secure_websocket": {
      "uri": "wss://echo.websocket.org",
      "message": "ping",
      "timeout": 15,
      "headers": {
        "User-Agent": "WebSocket-Probe/1.0"
      }
    },
    "authenticated_websocket": {
      "uri": "wss://api.example.com/ws",
      "message": "{\"type\": \"ping\", \"data\": {}}",
      "timeout": 10,
      "headers": {
        "Authorization": "Bearer your-token-here",
        "X-API-Key": "your-api-key-here"
      }
    },
    "chat_websocket": {
      "uri": "ws://localhost:3000/chat",
      "message": "{\"type\": \"message\", \"content\": \"Hello from probe!\", \"user\": \"probe\"}",
      "timeout": 5
    }
  },
  "test_servers": {
    "echo_server": "wss://echo.websocket.org",
    "websocket_king": "wss://websocket.king",
    "local_development": "ws://localhost:8080/ws"
  },
  "probe_settings": {
    "default_timeout": 10,
    "default_interval": 5,
    "default_concurrency": 3,
    "default_stress_count": 10
  }
}
